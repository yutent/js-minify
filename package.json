{
  "name": "js-minify",
  "displayName": "js-minify",
  "description": "自用的js压缩插件",
  "version": "1.0.3",
  "publisher": "yutent",
  "author": "Yutent [@yutent]",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./index",
  "contributes": {
    "commands": [
      {
        "command": "JSminify.compile",
        "title": "编译当前ES7..."
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "!inOutput",
          "command": "JSminify.compile",
          "title": "编译当前ES7..."
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "es7 to es5 编译配置",
      "properties": {
        "JSminify.compileOnSave": {
          "type": "boolean",
          "default": true,
          "description": "保存时自动编译"
        },
        "JSminify.outdir": {
          "type": "string",
          "default": "dist",
          "description": "是否压缩代码"
        },
        "JSminify.exclude": {
          "type": "string",
          "default": "",
          "description": "要忽略的目录/文件的正则"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yutent/es7-to-es5.git"
  },
  "scripts": {},
  "license": "MIT",
  "dependencies": {
    "iofs": "^1.2.1",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {}
}
